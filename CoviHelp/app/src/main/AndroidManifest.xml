<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covihelp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/helplogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/helplogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoviHelp">
        <activity android:name=".Login"></activity>
        <activity android:name=".Register" />
        <activity android:name=".LoginSignUp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HelpTheNeedy"></activity>
        <activity android:name=".AddHelpInfo" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>